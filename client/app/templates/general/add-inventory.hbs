<div class="row">
  <div class="col-xs-12">
    <div class="add-inven-message">
      <h4 class="text-center">Scan for or manually enter a barcode below</h4>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-12">
    <div class="form-group ">
      {{input type='text' value=id placeholder="Barcode"  style="width:100%;" focus-out="alertToNoItem" class="form-control input-lg"}}
    </div>
  </div>
</div>
<div class="alert alert-danger hidden no-item-found" role="alert">
  <h4>No Item Found</h4>
  <button class="btn btn-default" role="type" {{action "createNewItem"}}>Create Item</button>
  <button class="btn btn-default" role="type" {{action "cancelSearch"}}>Cancel Search</button>
</div>
<hr style="margin-top:10px;margin-bottom:10px">
<div class="container-fluid add-inventory-form">


    {{add-inventory addItem="addItem" checkForDetail="checkForDetail" sales_price=item.sales_price details=item.Details item=item alertToNoItem="alertToNoItem" findItem="setItem" book=book tagChoices=tagChoices illustrators=illustratorChoices writers=writerChoices name=item.name typeName=item.typeName gender=item.gender brand=item.brand price=item.price vol_num=item.vol_num bindingType=item.bindingType trade=item.trade apparel=item.apparel comic=item.comic includedIn=item.includedIn collects=item.collects figure=item.figure figureType=item.figureType materialType=item.materialType game=item.game gameType=item.gameType Illustrators=item.Illustrators Writers=item.Writers tags=item.tags}}



</div>
